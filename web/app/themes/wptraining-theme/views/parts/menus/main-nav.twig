<nav>
	<ul class="nav">
		{% for item in navbar_menu.items %}
			<li class="nav-item {{item.class}}">
				<a href="{{item.url}}" class="nav-link" {{item.is_target_blank ? 'target="_blank"'}}>{{item.title}}</a>
				{% if item.children %}
					<ul class="nav-drop">
						{% for child in item.children %}
							<li class="nav-drop-item">
								<a href="{{child.url}}" {{item.is_target_blank ? 'target="_blank"'}}>{{child.title}}</a>
							</li>
						{% endfor %}
					</ul>
				{% endif %}
			</li>
		{% endfor %}
	</ul>
</nav>
